%h2="Item List"
-# .dog => <div class="dog"></div>
%table
	%thead
		%tr
			%th Image
			%th Item Name
			%th Price
			%th Add To Cart
	%tbody
		-@store.getAllItems(@store.id).each do |i|
			%tr
				%td
					%img.p-1{alt: "", height: "75", width: "75", src: i.img_src}
				%td= i.name
				%td= i.price
				%td
					%a{href: "/add_item_to_bucket/#{i.id}"}
						%img.img-fluid.rounded.float-end{alt: "", height: "20", width: "20", src: i.getPlusIcon}